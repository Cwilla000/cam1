{
  "name": "illusionengine",
  "version": "1.0.0",
  "description": "",
  "main": "build/main.js",
  "scripts": {
    "watch": "node build.mjs --watch",
    "start:dev": "cross-env DEV=true concurrently -k \"npm:serve\" \"wait-on http://127.0.0.1:3000 && npm run start:build\"",
    "start:build": "electron ./build/main.js",
    "serve": "live-server build --no-browser --port=3000 --host=127.0.0.1",
    "typecheck": "tsc --no-emit",
    "build": "node build.mjs && electron-builder"
  },
  "build": {
    "productName": "IllusionEngine",
    "files": [
      "./build/**/*"
    ]
  },
  "keywords": [],
