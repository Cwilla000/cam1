    }
  ],
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "concurrently": "^8.2.2",
    "electron": "^29.1.6",
    "electron-builder": "^24.13.3",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "live-server": "^1.2.2",
    "typescript": "^5.4.3",
    "wait-on": "^7.2.0"
  },
